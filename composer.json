{
    "name": "ghjayce/phparm",
    "description": "Sharp Code, In Your Hands.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "utility",
        "utilities",
        "utils",
        "utility-library",
        "utility-function"
    ],
    "authors": [
        {
            "name": "GHJayce",
            "email": "jaycedeng@outlook.com",
            "homepage": "https://github.com/GHJayce",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/GHJayce/phparm",
    "support": {
        "issues": "https://github.com/GHJayce/phparm/issues",
        "docs": "https://ghjayce.github.io/phparm"
    },
    "require": {
        "illuminate/contracts": "^8.83",
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symplify/monorepo-builder": "^11.2"
    },
    "replace": {
        "ghjayce/phparm-entity": "self.version",
        "ghjayce/phparm-url": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Ghjayce\\PhparmEntity\\": "src/Entity/src/",
            "Ghjayce\\PhparmUrl\\": "src/Url/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GhjayceTest\\PhparmEntity\\": "src/Entity/tests/",
            "GhjayceTest\\PhparmUrl\\": "src/Url/tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }
}
